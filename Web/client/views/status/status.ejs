  <div id="map"></div>


   <!-- Right side section  -->
   <div id="over_map2" style="background-color: white;opacity: 0.8;padding: 20px; border-radius:10px;border: 2px solid #327ffc;">
      <p><input style="display: none;"type="checkbox" id="present" name="present" unchecked> Presentation Mode </p> 
      <p> Select Type of Proccessing </p>
      <input type="radio" id="type0" name="type" value="0" checked> Basic<br>
      <input type="radio" id="type1" name="type" value="1"> Node<br>
      <input type="radio" id="type2" name="type" value="2"> Link <br>
      <button onclick="linkErrorDown()"> Error Links </button>
      <button onclick='healthDown()'> Health Download</button>
      <button onclick='pirSiteDown()'> Error pir site</button>
      <!-- <button onclick='pirPopDown()'> Error pir Pop</button> -->

      <!-- <input type="radio" id="kpi3" name="kpi" value="3"> Maximum Active UEs <br> -->
      <div id='right'></div>
   </div>

     <div id="over_map" style="background-color: white;opacity: 0.7;padding-top:10px;padding-left: 20px;border-radius:10px;border: 2px solid #327ffc;">
      <input type="range" style="left: 5px" id="myRange" min="1" max="100" oninput="showVal(this.value)" onchange="showVal(this.value)">
      <p id="health"> Set Health </p>
      <input type="range" style="left: 5px" id="limit2" min="0" max="300000" oninput="showVal2(this.value)" onchange="showVal2(this.value)">
      <p id="limit"> Set Limit </p>
    <input type="radio" id="kpi0" name="kpi" value="0" checked> S1 Interface<br>
    <input type="radio" id="kpi1" name="kpi" value="1"> X2 Interface<br>
    <input type="radio" id="kpi2" name="kpi" value="2"> PDCP_DATA_RATE_MAX_DL <br>
    <input type="radio" id="kpi3" name="kpi" value="3"> Maximum Active UEs <br>
    <input type="checkbox" id="fdd" checked> Is FDD<br>
   </div>

   <div id = "over_map3" style="background-color: white;opacity: 0.7;border-radius:10px;border: 2px solid #327ffc;padding: 20px">
     <input type="text" id="site" placeholder="Search Site by id" onkeyup="mySite(event)">
     
     <input type="text" id="box" placeholder="Enter Max Occupancy Index" onkeyup="mySite1(event)">
       <input type="text" id="box2" placeholder="Enter PIR" onkeyup="mySite2(event)">
     <div id='adderTo'></div>
   </div>

 </div>  
